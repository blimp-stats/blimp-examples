computeD2 <- function(chis, df){
k <- df
M <- length(chis)
sqrtchi <- sqrt(chis)
ARIV <- (1 + 1/M) * var(sqrtchi)
chimean <- mean(chis)
Tf <- (chimean/k - ((M+1)/(M-1) * ARIV)) / (1 + ARIV)
Tc <- (chimean - ((M+1)/(M-1) * ARIV * k)) / (1 + ARIV)
dfD = k**-(3/M) * (M - 1) * (1 + 1 / ARIV)**2
pF <- 1 - pf(Tf, k, dfD)
pchi <- 1 - pchisq(Tc,k)
results <- c(Tf,ARIV,k,dfD,pF,Tc,pchi)
return(results)
}
