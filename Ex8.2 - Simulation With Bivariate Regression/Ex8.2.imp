SIMULATE: 
	# Sample Size
	n = 1000;
	# Define parameters for model
	define:
		cor = 0.3;
		b0  = 10;
		b1  = 0.5;
		b2  = 0.3;
		s2e = 1.0;
	n: 
		# Create phantom variable
		u = normal(0, cor);
		# Generate correlated x1 and x2 ~ normal(0, 1)
		x1:x2 = normal(u, 1 - cor);
		# Generate y
		y = normal(b0 + b1*x1 + b2*x2, s2e);
VARIABLES: x1:x2 y; # Save only x1, x2, and y
MODEL: 
	1: y ~ x1:x2;
	2: x1 ~~ x2;
BURN: 5000;
ITER: 5000;
SEED: 10972;
SAVE:
	dataset = generated_data.csv;