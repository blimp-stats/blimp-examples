DATA: data4.dat;
VARIABLES: id a1:a3 v1 y m v2 x v3:v22;
MISSING: 999;
TRANSFORM:
m.mis = ismissing(m);
y.mis = ismissing(y);
MODEL: 
mediation.model:
m ~ x@alpha;
y ~ m@beta x;
missingness.model:
m.mis ~ m x;
y.mis ~ y m x;
auxiliary.model:
# sequential specification for auxiliary variables
a1:a3 ~ y m x;
PARAMETERS:
indirect = alpha * beta;
SEED: 90291;
BURN: 2000;
ITERATIONS: 10000;